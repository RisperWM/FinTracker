{
  "expo": {
    "name": "FinTracker",
    "slug": "FinTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fintracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.risperwm.fintracker",
      "icon": "./assets/images/icon.png"
    },
    "android": {
      "package": "com.risperwm.fintracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#fff"
      },
      "edgeToEdgeEnabled": true,
      "backgroundColor": "#fff"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fintracker.png",
          "resizeMode": "cover",
          "backgroundColor": "#fff",
          "enableFullScreenImage_legacy": true,
          "dark": {
            "image": "./assets/images/fintracker.png",
            "backgroundColor": "#fff"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e7be9a42-df10-41ff-8a4d-8ec134bc93af"
      }
    }
  }
}
